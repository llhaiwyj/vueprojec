	}
	
	.jilubox {
		padding-left: 10px;
		padding-right: 10px;
	}
	
	.julist:nth-child(1) {
		padding: 10px 0;
		height: 25px;
		line-height: 25px;
	}
	
	.julist {
		padding: 4px 0;
		height: 25px;
		line-height: 25px;
	}
	
	.jilu {
		font-size: 15px;
		text-align: left;
		float: left;
		display: inline-block;
	}
	
	.gengduo {
		text-align: left;
		float: right;
		display: block;
		border: 1px solid #fff;
		height: 20px;
		line-height: 20px;
		padding: 0 10px;
	}
	
	.gengduo a {
		color: #FFFFFF;
		text-align: center;
		font-size: 12px;
		display: block;
		height: 20px;
		line-height: 20px;
	}
	
	.servername {
		font-size: 12px;
		text-align: left;
		float: left;
		display: inline-block;
		color: #fff;
		width: 70%;
		white-space: nowrap;
		overflow: hidden;
		text-overflow: ellipsis;
	}
	
	.serverdata {
		font-size: 12px;
		color: #fff;
		text-align: left;
		float: right;
		display: block;
	}