	.individualorder{
		width: 70%;
		height: 504px;
		margin: 15px auto;
		box-shadow:0px 2px 2px 0px rgba(0,0,0,.4);		
		border-radius: 8px;
		margin-left:10px;
	}
	.individualorder .el-tabs--border-card{
		background: #fff;
       border-bottom: 1px solid #dcdfe6;
    -webkit-box-shadow:  none;
     box-shadow: none; 
     border: 0 !important;
	}
	.individualorder .el-tabs__nav-scroll{
		height: 55px;
		line-height: 55px;
		background: #fff;
		 border-bottom: 1px solid #dcdfe6;
	}
	.individualorder .el-tabs--border-card>.el-tabs__header .el-tabs__item.is-active{
		color: #409EFF !important;
	    background-color: transparent;
	    border-right-color:0;
	    border-left-color: 0;
	    border-right: 0;
	    border-left:0;
	    border-bottom: 2px solid #409EFF;
	}
	.individualorder .el-tabs--border-card>.el-tabs__header .el-tabs__item{
		transition: none;
	     border:0 transparent; 
	    margin: 0;
	    color: #909399;
	}
	.individualorder .el-tabs__nav{
		border:0 !important;
	}
